 name: 'Events',\n  data() {\n    return {\n      imgs: [\"test_1.jpg\", \"test.jpg\", \"cornell.png\"],\n      timer: null,\n      currentIndex: 0\n    };\n  },\n  props: {\n    msg: String\n  },\n  components: {\n    'navi' : navi\n  },\n  methods: {\n    next: function() {\n      this.currentIndex += 1;\n    },\n    prev : function() {\n      this.currentIndex -= 1;\n    },\n    incr : function() {\n      this.currentIndex += 1;\n    }\n  },\n  computed: {\n    currentImg: function() {\n      return require('../assets/img/' + this.imgs[Math.abs(this.currentIndex) % this.imgs.length]);\n    }\n  }\n};\n</script>\n\n<!-- Add \"scoped\" attribute to limit CSS to this component only -->\n<style scoped>\n.container {\n  margin: auto;\n  width: 80%;\n  padding: 5%;\n  display: flex;\n  justify-content: center;\n  vertical-align: center;\n}\n.con {\n  margin: 5%;\n}\n.img_win {\n  background: rgb(241, 239, 239);\n  border-radius: 25px;\n  height: 500px;\n  width: 80%;\n  float: left;\n  box-shadow: 0 12px 16px 0 rgba(0,0,0,0.24), 0 17px 50px 0 rgba(0,0,0,0.19);\n}\n.text_win {\n  background: rgb(241, 239, 239);\n  border-radius: 25px;\n  height: 400px;\n  width: 50%;\n  box-shadow: 0 12px 16px 0 rgba(0,0,0,0.24), 0 17px 50px 0 rgba(0,0,0,0.19);\n}\n.slider {\n  margin: 0 auto;\n  margin-top: 30px;\n  width: 90%;\n  display: flex;\n  align-items: center;\n  justify-content: center;\n  border: 3px solid green;\n}\n.im {\n  margin-left: 2px;\n  margin-right: 2px;\n  height: 20%;\n  width: 20%;\n}\n\n.prev, .next {\n  cursor: pointer;\n  top: 40%;\n  width: auto;\n  padding: 16px;\n  color: black;\n  font-weight: bold;\n  font-size: 15px;\n  transition: 0.7s ease;\n  border-radius: 15px;\n  text-decoration: none;\n  user-select: none;\n}\n\n.next {\n  right: 0;\n}\n\n.prev {\n  left: 0;\n}\n\n.prev:hover, .next:hover {\n  border: 4px solid rgb(105, 104, 104);\n}\n</style>\n"],"sourceRoot":""}]}